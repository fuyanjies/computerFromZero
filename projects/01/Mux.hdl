/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=nel);
    And(a=a, b=nel, out=A);
    And(a=b, b=sel, out=B);
    Or(a=A, b=B, out=out);
}
